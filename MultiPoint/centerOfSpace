%calculates center of space
function centerOfSpace = centerOfSpace(network)
    sumX = 0;
    sumY = 0;
    for P = network.pnts
        sumX = sumX + P.loc(1);
        sumY = sumY + P.loc(2);
        avgX = 1:length(P)
        avgY = 1:length(P)
    end
    centerOfSpace = [sumX/avgX sumY/avgY];
end
